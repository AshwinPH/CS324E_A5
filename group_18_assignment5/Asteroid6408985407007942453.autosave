class Asteroid{
    float r;
    float theta;
    float v;
    int n;
    float f;
    PShape rock;
    
    Asteroid(float r, float theta, float v, int n, float f, PShape rock){
      this.r = r;
      this.theta = theta;
      this.v = v;
      this.n = n;
      this. f = f;
      this.rock = rock;
    }
    
    void create(){
      rock = createShape(SPHERE,100);
    }
  
  
  
}
